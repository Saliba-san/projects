buffer	gnuplot.h	/^    std::vector<std::string> buffer;$/;"	m	class:GnuplotPipe
main	test.cpp	/^int main(){$/;"	f
pipe	gnuplot.h	/^    FILE* pipe;$/;"	m	class:GnuplotPipe
sendEndOfData	gnuplot.h	/^    void sendEndOfData(unsigned repeatBuffer = 1){$/;"	f	class:GnuplotPipe
sendLine	gnuplot.h	/^    void sendLine(const std::string& text, bool useBuffer = false){$/;"	f	class:GnuplotPipe
sendNewDataBlock	gnuplot.h	/^    void sendNewDataBlock(){$/;"	f	class:GnuplotPipe
writeBufferToFile	gnuplot.h	/^    void writeBufferToFile(const std::string& fileName){$/;"	f	class:GnuplotPipe
